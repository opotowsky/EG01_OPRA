<My_include>
  <facility>
    <name>LWR_storage</name>
    <config>
      <Storage>
        <in_commods> 
          <val>SpentUOX_cooled</val> 
        </in_commods>
        <out_commods> 
          <val>SpentUOX_stored</val> 
        </out_commods>
        <residence_time>0</residence_time>
      </Storage>
    </config>
  </facility>

  <facility>
    <name>LWR_cooling</name>
    <config>
      <Storage>
        <in_commods> 
<!--
          <val>SpentUOX</val>
-->
          <val>SpentUOX_NoAdditive</val> 
          <val>SpentUOX_Additive</val>
        </in_commods>
        <out_commods> 
          <val>SpentUOX_cooled</val> 
        </out_commods>
        <residence_time>81</residence_time>
      </Storage>
    </config>
  </facility>

  <facility>
    <name>LWR</name>
    <lifetime>720</lifetime>
    <config>
      <Reactor>
        <fuel_incommods> 
          <val>UOX_NoAdditive</val> 
          <val>UOX_Additive</val> 
        </fuel_incommods>
        <fuel_outcommods> 
          <val>SpentUOX_NoAdditive</val> 
          <val>SpentUOX_Additive</val> 
        </fuel_outcommods>
        <fuel_inrecipes> 
          <val>UOX_no232</val> 
          <val>UOX_232</val> 
        </fuel_inrecipes>
        <fuel_outrecipes> 
          <val>UOX_spent_no232</val> 
          <val>UOX_spent_232</val> 
        </fuel_outrecipes>
        <fuel_prefs>
          <val>1</val>
          <val>0.01</val>
        </fuel_prefs>
        <pref_change_times> 
          <val>24</val>
        </pref_change_times>
        <pref_change_commods> 
          <val>UOX_Additive</val> 
        </pref_change_commods>
        <pref_change_values> 
          <val>100</val> 
        </pref_change_values>
        <cycle_time>18</cycle_time>
        <refuel_time>0</refuel_time>
        <assem_size>29565</assem_size> 
        <n_assem_core>3</n_assem_core>
        <n_assem_batch>1</n_assem_batch>
        <power_cap>1000</power_cap> 
      </Reactor>
    </config>
  </facility>

